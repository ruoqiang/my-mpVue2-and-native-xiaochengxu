.contenter{
    position: relative;
    height:100%;
}
.contenter .title {
    line-height: 40px;
    height: 40px;
    font-size: 13px;
    padding-left: 14px;
    color: #333;
    position: fixed;
    z-index: 1;
    left: 0;
    top: 0;
    background: #f5f5f5;
    width: 100%;
}  
.h40{
    height: 40px;
}
.form-box:after {
    display: block;
    position: absolute;
    left: 0;
    bottom: 0;
    border-top: 1px solid #ddd;
    content: ' ';
    width: 100%;
  }
  .form-box:before {
    display: block;
    position: absolute;
    left: 0;
    top: 0;
    border-top: 1px solid #ddd;
    content: ' ';
    width: 100%;
  }
  .form-box .car-list {
    height: 70px;
    position: relative;
    margin-left: 14px;
    position: relative;
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    color: #333;
  }
  .form-box .car-list:after {
    display: block;
    position: absolute;
    left: 0;
    bottom: 0;
    border-top: 1px solid #ddd;
    content: ' ';
    width: 100%;
  }
  .form-box .car-list .car-img {
    width: 68px;
    height: 68px;
    margin-right: 20px;
    overflow: hidden;
    padding: 1px 0;
  }
  .form-box .car-list .car-img image {
    width: 100%;
    height: 100%;
  }
  .form-box .car-list .car-number {
    height: 70px;
    line-height: 70px;
  }
  .icon-more{
      position: absolute;
      right: 20px;
      top:24px;
      border: 1px solid #666;
      width: 10px;
      height: 10px;
      transform: skewX(10deg);
        border-left: none;
        border-top: none;
        transform: rotate(-45deg)
  }