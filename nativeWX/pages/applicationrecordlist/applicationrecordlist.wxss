/* pages/applicationrecordlist/applicationrecordlist.wxss */
.list-box-wrap {
    margin-top: 15px;
    padding: 0 10px;
    background: #f5f5f5;
  }
  .list-box-wrap .list-box {
    padding: 16px 14px;
    background: #fff;
    margin: 0 auto;
    position: relative;
    overflow: hidden;
    margin-bottom: 15px;
  }
  .list-box-wrap .list-box .list-car-info {
    position: relative;
  }
  .list-box-wrap .list-box .list-car-info view {
    width: 100%;
    text-align: left;
    font-size: 14px;
    color: #333;
    height: 20px;
    line-height: 20px;
    margin-bottom: 12px;
  }
  .list-box-wrap .list-box .list-car-info view text {
    display: inline-view;
  }
  .list-box-wrap .list-box .list-car-info view text:first-child {
    width: 91px;
  }
  .list-box-wrap .list-box .list-car-info view:last-child {
    margin-bottom: 0px;
  }
  .list-box-wrap .list-box .status-btn {
    position: absolute;
    right: -42px;
    top: 12px;
    font-size: 12px;
    color: #fff;
    background: #f7c223;
    height: 20px;
    width: 126px;
    line-height: 21px;
    -webkit-transform: rotate(45deg);
            transform: rotate(45deg);
            text-align: center;
  }
  .list-box-wrap .list-box .status-btn.success {
    background: #12c42b;
  }
  .list-box-wrap .list-box .status-btn.fail {
    background: #ff5b05;
  }