#record-car {
    position: fixed;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    background: #f5f5f5;
    text-align: left;
  }
  #record-car .record-img {
    height: 0px;
    padding-bottom: 87.9%;
    background: url("http://wechat.chepass.com:8021/Content/dist/static/img/record01.6bfdc7c.jpg") no-repeat;
    background-size: 100%;
    position: relative;
    overflow: hidden;
  }
  @media only screen and (min-width: 360px) {
  #record-car .record-img {
      padding-bottom: 79.9%;
  }
  }
  @media only screen and (min-width: 410px) {
  #record-car .record-img {
      padding-bottom: 75.9%;
  }
  }
  #record-car .record-img .record-time-text {
    width: 94.2%;
    height: 165px;
    position: absolute;
    left: 50%;
    bottom: 0;
    background: #fff;
    border: 1px solid #ddd;
    -webkit-transform: translate3d(-50%, 0, 0);
            transform: translate3d(-50%, 0, 0);
    border-radius: 5px 5px 0 0;
  }
  #record-car .record-img .record-time-text .title {
    line-height: 42px;
    height: 42px;
    padding-left: 15px;
    color: #333;
    font-size: 13px;
  }
  #record-car .record-img .record-time-text .time-text {
    width: 306px;
    margin: 0 auto;
    line-height: 42px;
    height: 42px;
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-pack: justify;
        -ms-flex-pack: justify;
            justify-content: space-between;
  }
  #record-car .record-img .record-time-text .time-text text {
    font-size: 13px;
    color: #333;
    display: inline-block;
  }
  #record-car .record-img .record-time-text .time-text text.time {
    font-size: 15px;
    width: 129px;
    position: relative;
    text-align: center;
  }

  #record-car .record-img .record-time-text .time-text text.time:after {
    display: block;
    position: absolute;
    left: 0;
    bottom: 0;
    border-top: 1px solid #ddd;
    content: ' ';
    width: 100%;
  }
  .time-text pick{
      position: relative;
  }
 .time-text pick:after {
    display: block;
    position: absolute;
    left: 0;
    bottom: 0;
    border-top: 1px solid #ddd;
    content: ' ';
    width: 100%;
  }
  #record-car .record-img .record-time-text .btn {
    margin-top: 25px;
  }
  .icon-top:before {
    position: absolute;
    top: 7px;
    left: 6px;
  }
  .record-list-box-wrap {
    position: relative;
  }
  .record-list-box-wrap .ajaxLoaing {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    background: #f5f5f5;
    padding-top: 30px;
    height: 100%;
  }
  .mask {
    position: absolute;
    left: 0;
    top: 0;
    width: 100%;
    height: 100%;
  }
  .btnWrap{
      padding: 0 15px;
  }
  .btnWrap .button{
    padding: 10px 0;
    text-align: center;
    color: #fff;
    background: #2d4ed1;
    border-radius: 3px;
    font-size: 16px;
}
/* .text,text{font-size: 14px;font-style: normal;} */
  .btnWrap .button:active,.btnWrap .button:focus{background: #2d4ed1;opacity: 0.9;}


  #record-list {
    height: 100%
}

.record-list-box {
    background: #f5f5f5;
    margin-top: 15px
}

.record-list-box .record-list {
    background: #fff;
    position: relative;
    color: #333;
    margin-bottom: 10px
}

.record-list-box .record-list:after {
    display: block;
    position: absolute;
    left: 0;
    bottom: 0;
    border-top: 1px solid #ddd;
    content: " ";
    width: 100%
}

.record-list-box .record-list:before {
    display: block;
    position: absolute;
    left: 0;
    top: 0;
    border-top: 1px solid #ddd;
    content: " ";
    width: 100%
}

.record-list-box .record-list .record-list-title {
    margin-left: 14px;
    line-height: 38px;
    height: 38px;
    margin-right: 20px;
    overflow: hidden;
    padding: 1px 0;
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-pack: justify;
    -ms-flex-pack: justify;
    justify-content: space-between
}

.record-list-box .record-list .record-list-title view {
    font-size: 14px;
    color: #333
}

.record-list-box .record-list .record-list-title view.cost {
    color: #2d4ed1
}

.record-list-box .record-list .record-list-info {
    position: relative;
    margin-left: 14px;
    padding: 7px 16px 7px 0
}

.record-list-box .record-list .record-list-info:before {
    display: block;
    position: absolute;
    left: 0;
    top: 0;
    border-top: 1px solid #ddd;
    content: " ";
    width: 100%
}

.record-list-box .record-list .record-list-info .in-out-list {
    height: 22px;
    line-height: 22px;
    display: -webkit-box;
    display: -ms-flexbox;
    display: flex;
    -webkit-box-pack: justify;
    -ms-flex-pack: justify;
    justify-content: space-between
}

.record-list-box .record-list .record-list-info .in-out-list .go-site {
    font-size: 13px;
    color: #999
}

.record-list-box .record-list .record-list-info .in-out-list .go-site text {
    color: #090909
}

.record-list-box .record-list .record-list-info .in-out-list .go-time {
    font-size: 13px;
    color: #999
}

.record-list-box .tipsStatusBox1 {
    position: relative;
    padding-top: 65px;
    width: 100%
}

.seeMore {
    height: 40px;
    line-height: 40px;
    position: relative;
    width: 100%
}

.seeMore text {
    font-size: 14px;
    color: #333;
    position: relative
}

.noMore {
    position: relative
}

.noMore text {
    font-size: 14px
}

.noMore text:after {
    background: none
}

.dataTipsBtnBox {
    width: 100%;
    text-align: center;
    background: #fff;
    height: 40px;
    line-height: 40px;
}
.icon-top{
    position: fixed;
    right: 20px;
    bottom: 20px;
    width: 40px;
